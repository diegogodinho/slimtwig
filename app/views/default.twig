<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no"/>
        <!-- Apple devices fullscreen -->
        <meta name="apple-mobile-web-app-capable" content="yes"/>
        <!-- Apple devices fullscreen -->
        <meta names="apple-mobile-web-app-status-bar-style" content="black-translucent"/>

        <title>DSolutions -
            {% block title %}{% endblock %}
        </title>

        <link href="{{base_url()}}/app/template/interno/css/bootstrap.css" rel="stylesheet">
        <link href="{{base_url()}}/app/template/interno/css/plugins/jquery-ui/jquery-ui.css" rel="stylesheet">
        <link href="{{base_url()}}/app/template/interno/css/plugins/pageguide/pageguide.css" rel="stylesheet">
        <link href="{{base_url()}}/app/template/interno/css/plugins/fullcalendar/fullcalendar.css" rel="stylesheet">

        <link href="{{base_url()}}/app/template/interno/css/plugins/chosen/chosen.css" rel="stylesheet">
        <link href="{{base_url()}}/app/template/interno/css/plugins/select2/select2.css" rel="stylesheet">
        <link href="{{base_url()}}/app/template/interno/css/plugins/icheck/all.css" rel="stylesheet">
        <link href="{{base_url()}}/app/template/interno/css/style.css" rel="stylesheet">
        <link href="{{base_url()}}/app/template/interno/css/themes.css" rel="stylesheet">
        <link href="{{base_url()}}/app/template/interno/css/customizacoes.css" rel="stylesheet">
        <link href="{{base_url()}}/app/template/interno/css/plugins/datatable/TableTools.css" rel="stylesheet">
        <link href="{{base_url()}}/app/template/interno/css/plugins/gritter/jquery.gritter.css" rel="stylesheet">
        <link href="{{base_url()}}/app/template/interno/css/plugins/datepicker/datepicker.css" rel="stylesheet">

        <!--[if lte IE 9]> <script src="js/plugins/placeholder/jquery.placeholder.min.js"></script> <script> $(document).ready(function() { $('input, textarea').placeholder(); }); </script> <![endif]-->

        <link rel="shortcut icon" href="~/favicon.ico" type="image/x-icon"/>
        <!-- Apple devices Homescreen icon -->
        <link rel="apple-touch-icon-precomposed" href="img/apple-touch-icon-precomposed.png"/>

        {% block header %}{% endblock %}

    </head>
    <body>

        <div class="page-header custom-page-header">
            <div class="container-fluid ">
                <div class="row">
                    <div class="col-xs-4 col-sm-4 col-md-2 col-lg-1">
                        <img src="{{base_url()}}/uploads/fbb6e784ace61849.png" width="120" height="50"/>
                    </div>
                    {% set watermark = foto.GetUrlWaterMark() %}
                    {% if watermark %}
                        <div class="pull-right-logo">
                            <img src="{{watermark}}" width="140" height="50"/>
                        </div>
                    {% endif %}
                </div>

            </div>
        </div>

        {% include 'templates/navigation.twig' %}

        <div class="container-fluid" id="content">
            <div id="main" style="margin-left: 0px;">
                <div class="container-fluid">
                    <div class="page-header">
                        {# <div class="pull-left">
                            <h1></h1>
                        </div> #}
                        {# <div class="pull-right">
                            <ul class="stats">
                                <li class='lightred'>
                                    <i class="fa fa-calendar"></i>
                                    <div class="details">
                                        <span class="big" id="calendario-topo"></span>
                                        <span id="calendario-topo-diahora"></span>
                                    </div>
                                </li>
                            </ul>
                        </div> #}
                    </div>
                    <div class="breadcrumbs">
                        <ul>
                            <li>
                                <a href="{{path_for('home')}}">Home</a>
                            </li>
                            {% block breadcrumbs %}{% endblock %}

                        </div>
                        {% block content %}{% endblock %}
                    </div>
                </div>
            </div>
            <div id="modal-validation-errors" class="modal fade" tabindex="-1" role="dialog">
                <div class="modal-dialog" role="document">
                    <div class="modal-content">
                        <div class="modal-header">
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span></button>
                            <h4 class="modal-title">Information</h4>
                        </div>
                        <div class="modal-body">
                            {% for erro in errors %}
                                {% for item in erro %}
                                    <li>{{ item }}</li>
                                {% endfor %}
                            {% endfor %}
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-success" data-dismiss="modal">Entendi!</button>
                        </div>
                    </div>
                    <!-- /.modal-content -->
                </div>
                <!-- /.modal-dialog -->
            </div>
            <!-- /.modal -->
            <script src="{{base_url()}}/app/template/jquery-2.1.4.js"></script>
            <script src="{{base_url()}}/app/template/jquery-ui.js"></script>

            <script src="{{base_url()}}/app/template/bootstrap.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/nicescroll/jquery.nicescroll.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/touch-punch/jquery.touch-punch.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/slimscroll/jquery.slimscroll.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/vmap/jquery.vmap.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/vmap/jquery.vmap.sampledata.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/flot/jquery.flot.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/flot/jquery.flot.bar.order.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/flot/jquery.flot.pie.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/flot/jquery.flot.resize.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/imagesLoaded/jquery.imagesloaded.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/pageguide/jquery.pageguide.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/fullcalendar/moment.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/fullcalendar/fullcalendar.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/chosen/chosen.jquery.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/select2/select2.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/plugins/icheck/jquery.icheck.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/eakroko.min.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/application.js"></script>
            <script src="{{base_url()}}/app/template/interno/js/demonstration.js"></script>
            <script src="{{base_url()}}/app/scriptsjs/DefaultDataTable.js"></script>
            

            <script>
                $(document).ajaxError(function (event, jqxhr, settings, thrownError) {
                    if (jqxhr.status == 403) {
                        window.location.href = "{{ path_for('login') }}";
                    }
                });

                function GetVersion() {
                    if ($("#versao").text() == "") {
                        $.get(window.location.origin + '/Base/VersionApp/', function (result) {
                            $("#versao").text(result);
                            $('#modal-versao').modal('show');
                        });
                    } else 
                        $('#modal-versao').modal('show');
                    }
                
                $(function () {});
            </script>

            {% block script %}{% endblock %}
        </body>
    </html>